"%.2f (astronómical)\n"
"\n"

#: moon.py:254
#, python-format
msgid ""
"Age:\n"
"%(days).0f days, %(hours).0f hours, %(minutes).0f minutes\n"
"\n"
msgstr ""
"Mara chimpu:\n"
"%(days).0f días, %(hours).0f horas, %(minutes).0f minutos\n"
"\n"

#: moon.py:255
#, python-format
msgid ""
"Lunation:\n"
"%(phase).2f%% through lunation %(lunation)d\n"
"\n"
msgstr ""
"Phaxsi pacha chimpu:\n"
"%(phase).2f%% phaxsi pacha chimpukama %(lunation)d\n"
"\n"

#: moon.py:256
#, python-format
msgid ""
"Surface Visibility:\n"
"%.0f%% (estimated)\n"
"\n"
msgstr ""
"Uraqi pacha uñjäwi:\n"
"%.0f%% (estimada)\n"
"\n"

#: moon.py:257
#, python-format
msgid ""
"Selenographic Terminator Longitude:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"
msgstr ""
"Terminador selenográfico phaxsi ampsta tañapa:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"

