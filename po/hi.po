#: ../moon.py:190
msgid ""
"Today's Moon Information\n"
"\n"
msgstr ""
"आज के चन्द्रमा की जानकारी\n"
"\n"

#: ../moon.py:191
#, python-format
msgid ""
"Phase:\n"
"%s\n"
"\n"
msgstr ""
"चन्द्राकार:\n"
"%s\n"
"\n"

#: ../moon.py:192
#, python-format
msgid ""
"Julian Date:\n"
"%.2f (astronomical)\n"
"\n"
msgstr ""
"जूलियन तिथि:\n"
"%.2f (खगोलीय)\n"
"\n"

#: ../moon.py:193
#, python-format
msgid ""
"Age:\n"
"%(days).0f days, %(hours).0f hours, %(minutes).0f minutes\n"
"\n"
msgstr ""
"उम्र:\n"
"%(days).0f दिन, %(hours).0f घंटे, %(minutes).0f मिनट\n"
"\n"

#: ../moon.py:194
#, python-format
msgid ""
"Lunation:\n"
"%(phase).2f%% through lunation %(lunation)d\n"
"\n"
msgstr ""
"ल्यूनेशन:\n"
"%(phase).2f%% ल्यूनेशन द्वारा %(lunation)d\n"
"\n"

#: ../moon.py:195
#, python-format
msgid ""
"Surface Visibility:\n"
"%.0f%% (estimated)\n"
"\n"
msgstr ""
"सतह की दृश्यता:\n"
"%.0f%% (प्राक्कलित)\n"
"\n"

#: ../moon.py:196
#, python-format
msgid ""
"Selenographic Terminator Longitude:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"
msgstr ""
"सेलेनोग्राफिक टर्मिनेटर लाँगीट्यूड:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"

#: ../moon.py:197
#, python-format
msgid ""
"Next Full Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"अगली पूर्णमासी:\n"
"%(date)s in %(days).0f days\n"
"\n"

#: ../moon.py:198
#, python-format
msgid ""
"Next New Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"अगला चन्द्रोदय:\n"
"%(date)s में से %(days).0f दिन\n"
"\n"

#: ../moon.py:199
#, python-format
msgid ""
"Next Lunar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"अगला चन्द्रग्रहण:\n"
"%(date)s में %(days).0f दिन\n"
"\n"

#: ../moon.py:200
#, python-format
msgid ""
"Next Solar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"अगला सूर्यग्रहण:\n"
"%(date)s में %(days).0f दिन\n"
"\n"

