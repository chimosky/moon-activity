#, python-format
msgid ""
"Age:\n"
"%(days).0f days, %(hours).0f hours, %(minutes).0f minutes\n"
"\n"
msgstr ""
"Âge:\n"
"%(days).0f jours, %(hours).0f heures, %(minutes).0f minutes\n"
"\n"

