msgid "Latitude"
msgstr "அகலாங்கு"

#: moon.py:368
msgid "Longitude"
msgstr "நெட்டாங்கு"

#: moon.py:465
msgid "west"
msgstr "தெற்கு"

#: moon.py:467
msgid "east"
msgstr "கிழக்கு"

#: moon.py:470
msgid "Sunset"
msgstr "சூரியன் மறைவு"

#: moon.py:472
msgid "Sunrise"
msgstr "சூரிய உதயம்"

#: moon.py:486 moon.py:502
msgid "New Moon"
msgstr "புதிய நிலா"

#: moon.py:488
msgid "Waxing Crescent"
msgstr "வளர் பிறை"

#: moon.py:490
msgid "First Quarter"
msgstr "முதற் கால் பகுதி"

#: moon.py:492
msgid "Waxing Gibbous"
msgstr "தேய் பிறை"

#: moon.py:494
msgid "Full Moon"
msgstr "முழு நிலா"

#: moon.py:496
msgid "Waning Gibbous"
msgstr "தேய்பிறை"

#: moon.py:498
msgid "Last Quarter"
msgstr "இறுதி கால் பகுதி"

#: moon.py:500
msgid "Waning Crescent"
msgstr "அரை நிலவு"
