#: activity/activity.info:2 moon.py:72
msgid "Moon"
msgstr "நிலவு"

#: moon.py:86
msgid "Toggle Grid View"
msgstr "அடைப்புப்பார்வைக்கு மாற்று"

#: moon.py:91
msgid "Toggle Hemisphere View"
msgstr "அரைப் பார்வைக்கு மாற்று"

#: moon.py:96
msgid "View"
msgstr "பார்வை"

#: moon.py:218
msgid ""
"Today's Moon Information\n"
"\n"
msgstr "இன்றைய நிலவின் தகவல் \n"

#: moon.py:219
#, python-format
msgid ""
"Phase:\n"
"%s\n"
"\n"
msgstr ""
"கட்டம்:\n"
"%\n"
"\n"

#: moon.py:220
#, python-format
msgid ""
"Julian Date:\n"
"%.2f (astronomical)\n"
"\n"
msgstr ""
"Julian திகதி:\n"
"%.2f (வானியல்)\n"

#: moon.py:221
#, python-format
msgid ""
"Age:\n"
"%(days).0f days, %(hours).0f hours, %(minutes).0f minutes\n"
"\n"
msgstr ""
"வயது:\n"
"%(நாட்கள்). நாட்கள், %(மணித்தியாலம்). மணித்தியாலம், %(நிமிடங்கள்). "
"நிமிடங்கள்\n"
"\n"

#: moon.py:222
#, python-format
msgid ""
"Lunation:\n"
"%(phase).2f%% through lunation %(lunation)d\n"
"\n"
msgstr ""
"Lunation:\n"
"%(phase).2f%% through lunation %(lunation)d\n"
"\n"

#: moon.py:223
#, python-format
msgid ""
"Surface Visibility:\n"
"%.0f%% (estimated)\n"
"\n"
msgstr ""
"மேல் பரப்பு பார்வை:\n"
"%.0f%% (மதிப்பிடப்பட்டது)\n"
"\n"

#: moon.py:224
#, python-format
msgid ""
"Selenographic Terminator Longitude:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"
msgstr ""
"Selenographic Terminator Longitude:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"

#: moon.py:225
#, python-format
msgid ""
"Next Full Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"அடுத்த முழு நிலவு:\n"
"%(நாள்)கள் %(நாட்கள்).நாட்களின்\n"
"\n"

#: moon.py:226
#, python-format
msgid ""
"Next New Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"அடுத்த புதிய நிலவு:\n"
"%(நாள்)கள் %(நாட்கள்).நாட்களின்\n"
"\n"

#: moon.py:227
#, python-format
msgid ""
"Next Lunar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"அடுத்த சந்திர கிரகணம்:\n"
"%(நாள்)கள் %(நாட்கள்).நாட்களின்\n"
"\n"

#: moon.py:228
#, python-format
msgid ""
"Next Solar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"அடுத்த சூரிய கிரகணம்:\n"
"%(நாள்)கள் %(நாட்கள்).நாட்களின்\n"
"\n"

