#: moon.py:250
msgid ""
"Today's Moon Information\n"
"\n"
msgstr ""
"Enfòmasyon sou lalin jodi a\n"
"\n"

#: moon.py:251
#, python-format
msgid ""
"Phase:\n"
"%s\n"
"\n"
msgstr ""
"Faz:\n"
"%s\n"
"\n"

#: moon.py:252
#, python-format
msgid ""
"Julian Date:\n"
"%.2f (astronomical)\n"
"\n"
msgstr ""
"Dat Jilyan:\n"
"%.2f (Astwonomik)\n"
"\n"

#: moon.py:253
#, python-format
msgid ""
"Age:\n"
"%(days).0f days, %(hours).0f hours, %(minutes).0f minutes\n"
"\n"
msgstr ""
"Aj:\n"
"%(days).0f jou yo, %(hours).0f è yo, %(minutes).0f minit yo\n"
"\n"

#: moon.py:254
#, python-format
msgid ""
"Lunation:\n"
"%(phase).2f%% through lunation %(lunation)d\n"
"\n"
msgstr ""
"Linezon:\n"
"%(phase).2f%% atravè linezon%(lunation)d\n"
"\n"

#: moon.py:255
#, python-format
msgid ""
"Surface Visibility:\n"
"%.0f%% (estimated)\n"
"\n"
msgstr ""
"Sifas Vizib:\n"
"%.0f%% (estime)\n"
"\n"

#: moon.py:256
#, python-format
msgid ""
"Selenographic Terminator Longitude:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"
msgstr ""
"Selenografik Lonjitid tèminal:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"

#: moon.py:257
#, python-format
msgid ""
"Next Full Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Pwochèn plèn lin:\n"
"%(date)s dan %(days).0f jou yo\n"
"\n"

#: moon.py:258
#, python-format
msgid ""
"Next New Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Pwochèn nouvèl lin:\n"
"%(date)s dan %(days).0f jou yo\n"
"\n"

#: moon.py:259
#, python-format
msgid ""
"Next Lunar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"pwochèn eklips lalin:\n"
"%(date)s dan %(days).0f jou yo\n"
"\n"

#: moon.py:260
#, python-format
msgid ""
"Next Solar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Pwochèn eklips solèy:\n"
"%(date)s dan %(days).0f jou yo\n"
"\n"

