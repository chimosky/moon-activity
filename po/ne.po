"\n"

#: moon.py:259
#, python-format
msgid ""
"Next Lunar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"यसपछिको शुक्ल ग्रह :\n"
"%(date)s को %(days).0f दिनहरु\n"
"\n"

#: moon.py:260
#, python-format
msgid ""
"Next Solar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"यसपछिको सौय ग्रह :\n"
"%(date)s को %(days).0f दिनहरु\n"
"\n"

