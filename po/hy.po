#: ../activity/activity.info:2
msgid "Moon"
msgstr "Լուսին"

#: ../moon.py:71
msgid "Moon"
msgstr "Լուսին"

#: ../moon.py:83
msgid "Toggle Grid View"
msgstr "Միացնել վանդակավոր տեսքը"

#: ../moon.py:88
msgid "Toggle Hemisphere View"
msgstr "Միացնել կիսագնդային տեսքը"

#: ../moon.py:93
msgid "View"
msgstr "Դիտել"

#: ../moon.py:190
msgid ""
"Today's Moon Information\n"
"\n"
msgstr ""
"Այսօրվա տեղեկատվությունը լուսնի մասին\n"
"\n"

#: ../moon.py:191
#, python-format
msgid ""
"Phase:\n"
"%s\n"
"\n"
msgstr ""
"Փուլը.\n"
"%s\n"
"\n"

#: ../moon.py:192
#, python-format
msgid ""
"Julian Date:\n"
"%.2f (astronomical)\n"
"\n"
msgstr ""
"Հուլիանոսյան ամսաթիվը.\n"
"%.2f (աստղագիտական)\n"
"\n"

#: ../moon.py:193
#, python-format
msgid ""
"Age:\n"
"%(days).0f days, %(hours).0f hours, %(minutes).0f minutes\n"
"\n"
msgstr ""
"Տարիքը.\n"
"%(days).0f օր, %(hours).0f ժամ, %(minutes).0f րոպե\n"
"\n"

#: ../moon.py:194
#, python-format
msgid ""
"Lunation:\n"
"%(phase).2f%% through lunation %(lunation)d\n"
"\n"
msgstr ""
"Լուսնային փուլ.\n"
"%(phase).2f%% լուսնային փուլով %(lunation)d\n"
"\n"

#: ../moon.py:195
#, python-format
msgid ""
"Surface Visibility:\n"
"%.0f%% (estimated)\n"
"\n"
msgstr ""
"Մակերեսային տեսանելիություն.\n"
"%.0f%% (ենթադրյալ)\n"
"\n"

#: ../moon.py:196
#, python-format
msgid ""
"Selenographic Terminator Longitude:\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"
msgstr ""
"Լուսնագրական տերմինատորի երկայնությունը.\n"
"%(deg).1f°%(westOrEast)s (%(riseOrSet)s)\n"
"\n"

#: ../moon.py:197
#, python-format
msgid ""
"Next Full Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Հաջորդ լիալուսինը.\n"
"%(date)s %(days).0f օր\n"
"\n"

#: ../moon.py:198
#, python-format
msgid ""
"Next New Moon:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Հաջորդ նորալուսինը.\n"
"%(date)s %(days).0f օր\n"
"\n"

#: ../moon.py:199
#, python-format
msgid ""
"Next Lunar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Հաջորդ լուսնային խավարումը.\n"
"%(date)s %(days).0f օր\n"
"\n"

#: ../moon.py:200
#, python-format
msgid ""
"Next Solar eclipse:\n"
"%(date)s in %(days).0f days\n"
"\n"
msgstr ""
"Հաջորդ արեւային խավարումը.\n"
"%(date)s %(days).0f օր\n"
"\n"

