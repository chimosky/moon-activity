msgid "Latitude"
msgstr "Լայնություն"

#: ../moon.py:340
msgid "Longitude"
msgstr "Երկայնություն"

#: ../moon.py:437
msgid "west"
msgstr "արեւմուտք"

#: ../moon.py:439
msgid "east"
msgstr "արեւելք"

#: ../moon.py:442
msgid "Sunset"
msgstr "Արեւամուտ"

#: ../moon.py:444
msgid "Sunrise"
msgstr "Արեւածագ"

#: ../moon.py:458 ../moon.py:474
msgid "New Moon"
msgstr "Նորալուսին"

#: ../moon.py:460
msgid "Waxing Crescent"
msgstr "Հասունացող կիսալուսին"

#: ../moon.py:462
msgid "First Quarter"
msgstr "Առաջին քառորդ"

#: ../moon.py:464
msgid "Waxing Gibbous"
msgstr "Հասունացող երեք քառորդ"

#: ../moon.py:466
msgid "Full Moon"
msgstr "Լիալուսին"

#: ../moon.py:468
msgid "Waning Gibbous"
msgstr "Խամրող երեք քառորդ"

#: ../moon.py:470
msgid "Last Quarter"
msgstr "Վերջին քառորդ"

#: ../moon.py:472
msgid "Waning Crescent"
msgstr "Խամրող կիսալուսին"
