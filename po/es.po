msgid "west"
msgstr "oeste"

#: moon.py:523
msgid "east"
msgstr "este"

#: moon.py:526
msgid "Sunset"
msgstr "Se pone"

#: moon.py:528
msgid "Sunrise"
msgstr "Sale"

#: moon.py:544 moon.py:560
msgid "New Moon"
msgstr "Luna Nueva"

#: moon.py:546
msgid "Waxing Crescent"
msgstr "Luna Nueva Visible"

#: moon.py:548
msgid "First Quarter"
msgstr "Cuarto Creciente"

#: moon.py:550
msgid "Waxing Gibbous"
msgstr "Luna Gibada Creciente"

#: moon.py:552
msgid "Full Moon"
msgstr "Luna Llena"

#: moon.py:554
msgid "Waning Gibbous"
msgstr "Luna Gibada Menguante"

#: moon.py:556
msgid "Last Quarter"
msgstr "Cuarto Menguante"

#: moon.py:558
msgid "Waning Crescent"
msgstr "Luna Menguante"
